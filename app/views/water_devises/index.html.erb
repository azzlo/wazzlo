<p id="notice"><%= notice %></p>

<h1>Water Devises</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Location</th>
      <th>State</th>
      <th>Lat</th>
      <th>Long</th>
      <th>Current value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @water_devises.each do |water_devise| %>
      <tr>
        <td><%= water_devise.name %></td>
        <td><%= water_devise.address %></td>
        <td><%= water_devise.location %></td>
        <td><%= water_devise.state %></td>
        <td><%= water_devise.lat %></td>
        <td><%= water_devise.long %></td>
        <td><%= water_devise.current_value %></td>
        <td><%= link_to 'Show', water_devise %></td>
        <td><%= link_to 'Edit', edit_water_devise_path(water_devise) %></td>
        <td><%= link_to 'Destroy', water_devise, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Water Devise', new_water_devise_path %>
